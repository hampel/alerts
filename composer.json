{
    "name": "hampel/alerts",
    "description": "Laravel package for managing session messages and alerts in view responses",
	"homepage" : "https://github.com/hampel/alerts",
	"authors": [{
		"name": "Simon Hampel",
		"email": "simon@hampelgroup.com",
		"role": "Developer"
	}],
	"support": {
		"issues": "https://github.com/hampel/alerts/issues",
		"source": "https://github.com/hampel/alerts"
	},
	"keywords" : [
		"laravel",
		"session",
		"alerts",
		"foundation",
		"bootstrap"
	],
	"license" : [
		"MIT"
	],
	"require": {
		"php": "^7.1.3|^8.0",
		"illuminate/support": "^8.0|^9.0|^10.0",
		"illuminate/session": "^8.0|^9.0|^10.0",
		"illuminate/translation": "^8.0|^9.0|^10.0"
	},
	"require-dev" : {
		"illuminate/view": "^8.0|^9.0|^10.0",
		"phpunit/phpunit": "^10.0",
		"mockery/mockery": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Hampel\\Alerts\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		},
		"laravel": {
			"providers": [
				"Hampel\\Alerts\\AlertServiceProvider"
			],
			"aliases": {
				"Alert": "Hampel\\Alerts\\Facades\\Alert"
			}
		}
	},
 	"minimum-stability": "dev",
	"prefer-stable": true
}
